\n        });\n      }, () => {}\n    )\n  }\n  render() {\n    return (\n      <section className={styles.Container}>\n        {[...this.state.Messages].map(e => e[1].Msg)}\n      </section>\n    );\n  }\n}\nexport default Messages;"]},"metadata":{},"sourceType":"module"}